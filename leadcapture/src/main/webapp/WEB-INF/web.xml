<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Leadcapture Application</display-name>

  <servlet>
    <servlet-name>ForwardingLeadCaptureServlet</servlet-name>
    <servlet-class>servlet.ForwardingLeadCaptureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ForwardingLeadCaptureServlet</servlet-name>
    <url-pattern>/ForwardingLeadCaptureServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>servlet.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/SignupServlet</url-pattern>
  </servlet-mapping>

</web-app>
